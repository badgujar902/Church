<script>
    $(document).ready(function () {
        debugger


       var Message=@(Html.Raw(Json.Encode(TempData["Message"])));
        var Icon=@(Html.Raw(Json.Encode(TempData["Icon"])));

        if(Message!=null)        {
            if(Icon!=null)
            {
                swal({
                    title:"",
                    text: Message,
                    type: Icon

                },
                function(){
                    window.location.href = '@Url.Action("FamilyMemberList", "AddFamilyMember")';
                }
                );
            }

        }

        $("#dllGender").select2();

           //Space Not Allowed
        $('#IND_Mob').on('input', function (e) {
            debugger;
            var input = $(this).val();
            if (e.which == 32) {
                swal("", "Space not Allowed", "error");
                return false;
            }
            if (e.which != 8 && e.which != 0 && (e.which < 48 || e.which > 57))
            {
                swal("", "Enter Only Digits", "error");
                return false;
            }

        });



        $('#IND_Email').on('keypress', function (e) {
            if (e.which == 32) {
                swal("", "Space not Allowed", "error");
                return false;
            }
            //else{
            //    $("#U_ID34").val(null);
            //    $("#U_Pass34").val(null);
            //}
        });

        $('#U_ID').on('keypress', function (e) {
            if (e.which == 32) {
                swal("", "Space not Allowed", "error");
                return false;
            }
        });

        $('#U_Pass').on('keypress', function (e) {
            if (e.which == 32) {
                swal("", "Space not Allowed", "error");
                return false;
            }

        });

        $('#AdharNo').on('keypress', function (e) {
            debugger

            if (e.which == 32) {
                swal("", "Space not Allowed", "error");
                return false;
            }
            if (e.which != 8 && e.which != 0 && (e.which < 48 || e.which > 57))
            {
                swal("", "Enter Only Digits", "error");
                return false;
            }


        });


    });//ready



    $("#btnSave").click(function(){
        debugger;   

        var INDVSLFID=$("#MemberFID").val();
        var UserName=$("#UserName").val();
        var UserPassword=$("#Password").val();
        $(".Userloaderbg").css("display", "flex");
        $.ajax({
            url: '@Url.Action("SaveRegistrationRequestApprove", "RegistrationRequestApprove")',
            method:"get",
            data:{INDVSLFID:INDVSLFID,UserName:UserName,UserPassword:UserPassword},
            success:function(response){
                $(".Userloaderbg").css("display", "none");
                debugger;
                var Message1 = response.Message;
                var Icon1 = response.Icon;
                if (Message1 != null) {
                    Swal.fire({
                        title: '',
                        text: Message1,
                        icon: Icon1,
                        confirmButtonColor: '#3085d6',
                        confirmButtonText: 'Ok'
                    }).then((result) => {
                        if (result.isConfirmed) {                        
                            window.location.href = '@Url.Action("RegistrationList", "LeaderDashboard")';
                            console.log('User clicked confirm');
                        } else if (result.dismiss === Swal.DismissReason.cancel) {
                            // Handle when the user clicks cancel or closes the modal
                            console.log('User clicked cancel or closed the modal');
                        }
                    });                 

                }
            }
        });

    });

</script>
<script>
    $('#ShowPassword').click(function() {
        debugger;
        var passwordInput = $('#Password');
        var buttonIcon = $('#ShowPassword i');

        // Toggle password visibility
        if (passwordInput.attr('type') === 'password') {
            passwordInput.attr('type', 'text');
            buttonIcon.removeClass('fa-eye').addClass('fa-eye-slash'); // Change icon to 'eye-slash' icon
        } else {
            passwordInput.attr('type', 'password');
            buttonIcon.removeClass('fa-eye-slash').addClass('fa-eye'); // Change icon back to 'eye' icon
        }
    })
</script>





