<script>
    $(document).ready(function () {
        $(".Userloaderbg").css("display", "none");

        $('#tblNoticeNotification').DataTable({

            scrollY: 300,
            scrollCollapse: true,
            paging: false,
            scrollX: true,
            searching: false, // Disable search bar
            ordering: false, // Disable sorting arrows
            info: false // Disable showing "Showing 1 to 4 of 4 entries"

        });
        $(function () {
            $('.txtDate').datepicker({
                format: "dd/mm/yyyy"
            });
        });

        $(document).on("click", "#btnlogin", function () {
            Swal.fire({
                title: 'Are you sure',
                text: 'You want to logout?',
                icon: 'warning',
                showCancelButton: true,
                confirmButtonColor: '#3085d6',
                cancelButtonColor: '#ff002f',
                confirmButtonText: 'Yes'
            }).then((result) => {
                if (result.isConfirmed) {
                    //.fade:not(.show)
                    window.location.href = '@Url.Action("Login", "Home", new { area = "" })';
                    console.log('User clicked confirm');
                } else if (result.dismiss === Swal.DismissReason.cancel) {
                    console.log('User clicked cancel or closed the modal');
                }
            });
        });
    });

